{
  "name": "movie-tracker",
  "private": true,
  "type": "module",
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "dev": "vite --port 3000",
    "build": "vite build && tsc",
    "preview": "vite preview",
    "test": "vitest run",
    "lint": "eslint",
    "format": "prettier",
    "check": "prettier --write . && eslint --fix",
    "mobile:start": "cd packages/mobile && npx expo start --dev-client",
    "mobile:android": "cd packages/mobile && npx expo run:android",
    "mobile:ios": "cd packages/mobile && npx expo run:ios",
    "mobile:web": "cd packages/mobile && npx expo start --web",
    "mobile:lint": "cd packages/mobile && npx expo lint",
    "mobile:prebuild": "cd packages/mobile && npx expo prebuild",
    "mobile:build:dev": "cd packages/mobile && npx eas-cli@latest workflow:run create-development-builds.yml",
    "mobile:build:prod": "cd packages/mobile && npx eas-cli@latest workflow:run create-production-builds.yml",
    "web:dev": "cd packages/web && npx vite",
    "web:build": "cd packages/web && npx tsc && npx vite build",
    "web:preview": "cd packages/web && npx vite preview",
    "start:all": "concurrently \"yarn web:dev\" \"yarn mobile:android\"",
    "start:web+android": "concurrently \"yarn web:dev\" \"yarn mobile:android\""
  },
  "dependencies": {
    "@headlessui/react": "^2.2.9",
    "@heroicons/react": "^2.2.0",
    "@tailwindcss/vite": "^4.0.6",
    "@tanstack/react-devtools": "^0.7.0",
    "@tanstack/react-query": "^5.90.18",
    "@tanstack/react-router": "^1.132.0",
    "@tanstack/react-router-devtools": "^1.132.0",
    "@tanstack/router-plugin": "^1.132.0",
    "@workos-inc/authkit-react": "^0.13.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "lucide-react": "^0.544.0",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "react-simple-star-rating": "^5.1.7",
    "tailwind-merge": "^3.4.0",
    "tailwindcss": "^4.0.6",
    "tw-animate-css": "^1.3.6",
    "zod": "^4.1.11"
  },
  "devDependencies": {
    "@tanstack/devtools-vite": "^0.3.11",
    "@tanstack/eslint-config": "^0.3.0",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/react": "^16.2.0",
    "@types/node": "^22.10.2",
    "@types/react": "^19.2.0",
    "@types/react-dom": "^19.2.0",
    "@vitejs/plugin-react": "^5.0.4",
    "concurrently": "^9.2.1",
    "jsdom": "^27.0.0",
    "prettier": "^3.5.3",
    "typescript": "^5.7.2",
    "vite": "^7.1.7",
    "vitest": "^3.0.5",
    "web-vitals": "^5.1.0"
  },
  "packageManager": "yarn@4.12.0"
}
